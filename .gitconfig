[user]
	name = tosoegawa
	email = osoegawa@argano.co.jp
[init]
	defaultBranch = main
[core]
	editor = nvim
[pager]
  diff = delta
  log = delta
  reflog = delta
  show = delta
[delta]
  navigate = true
  line-numbers = true
  side-by-side = true
[interactive]
  diffFilter = delta --color-only
[alias]
	gr = log --oneline --all --graph --tags
	st = status
